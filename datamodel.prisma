enum Previledge {
  SUPERADMIN
  ADMIN
  MODERATOR
  AUTHOR
  READER
}

type Post {
  id: ID! @unique
  title: String!
  content: String!
  category: String!
  excerpt: String!
  updatedAt: DateTime!
  createdAt: DateTime!
  author: [User]!
}

type User {
  id: ID! @unique
  fname: String!
  lname: String!
  phone: String
  email: String!
  bio: String!
  posts: [Post]!
  followers: [User]!
  previledge: [Previledge]!
  updatedAt: DateTime!
  createdAt: DateTime!
}